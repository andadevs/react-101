    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "docdash": "^1.0.2",
    "eslint": "^5.12.0",
